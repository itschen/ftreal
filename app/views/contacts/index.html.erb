<table id='contact-list'>
  <tr class='titles'>
    <th>Last name</th>
    <th>First name</th>
    <th>Company name</th>
    <th>Title</th>
    <th>Email</th>
    <th>Telephone</th>
    <th>Mobile</th>
    <th>Fax</th>
	<th>Full Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%=h contact.last_name %></td>
    <td><%=h contact.first_name %></td>
    <td><%=h contact.company_name %></td>
    <td><%=h contact.title %></td>
    <td><%=h contact.email %></td>
    <td><%=h contact.telephone %></td>
    <td><%=h contact.mobile %></td>
    <td><%=h contact.fax %></td>
	<td><%=h contact.full_address %></td>
    <td><%=h contact.city %></td>
    <td><%=h contact.state %></td>
    <td><%=h contact.zip %></td>

    <td class="list-actions">
		<%= link_to 'Edit', edit_contact_path(contact) %></br>
        <%= link_to 'Destroy', contact, :confirm => 'Are you sure?', :method => :delete %>
	</td>
	
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New contact', new_contact_path %>